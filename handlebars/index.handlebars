<!DOCTYPE html>
<html lang="en">

{{>head}}

<body>
	<!-- s: root -->
	<div class="root">

		<!-- homepage -->
		<main class="main">

			<div class="main-ornament -one picturefill-background">
				<span data-src="asset/images/main-graphic-elements-top-m.png"></span>
				<span data-src="asset/images/main-graphic-elements-top-t.png" data-media="(min-width: 768px)"></span>
				<span data-src="asset/images/main-graphic-elements-left.png" data-media="(min-width: 1200px)"></span>
			</div>
			<div class="main-ornament -two picturefill-background">
				<span data-src="asset/images/main-graphic-elements-bottom-m.png"></span>
				<span data-src="asset/images/main-graphic-elements-bottom-t.png" data-media="(min-width: 768px)"></span>
				<span data-src="asset/images/main-graphic-elements-right.png" data-media="(min-width: 1200px)"></span>
			</div>

			<div class="main-img picturefill-background">
				<span data-src="asset/images/main-hand-illustration-m.png"></span>
				<span data-src="asset/images/main-hand-illustration.png" data-media="(min-width: 990px)"></span>
			</div>

			<div class="main-home">
				<div class="main-logo">
					<img src="asset/images/byu-logo.svg" alt="">
